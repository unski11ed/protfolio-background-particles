{
  "name": "rippled-particles",
  "version": "1.0.0",
  "description": "Creates an effect where particles with defined parameters are simulated on the provided Canvas element. A ripple may be triggered from a specific origin location to create a cool transition effect.",
  "main": "dist/cjs/rippled-particles.js",
  "module": "dist/es/rippled-particles.mjs",
  "browser": "dist/browser/rippled-particles.js",
  "types": "dist/ts/rippled-particles.d.ts",
  "scripts": {
    "start": "parcel --out-dir=.temp src/example/index.html",
    "build": "rollup -c"
  },
  "dependencies": {
    "@types/eases": "^1.0.0",
    "@types/eventemitter3": "^2.0.2",
    "@types/lodash": "^4.14.136",
    "eases": "^1.0.8",
    "eventemitter3": "^4.0.0",
    "fps-throttler": "^1.0.2",
    "lodash": "4.17.15",
    "reset-css": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "parcel-bundler": "^1.6.1",
    "rollup": "^1.18.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "typescript": "^3.5.3"
  }
}
